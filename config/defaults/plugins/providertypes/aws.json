                    "override": false,
                    "sensitive": false,
                    "tip": "AWS security group (non-VPC)",
                    "type": "text"
                },
                "subnet_id": {
                    "label": "Subnet ID (VPC only)",
                    "override": false,
                    "sensitive": false,
                    "tip": "Enable VPC and use this subnet ID",
                    "type": "text"
                }
            },
            "required": [
                [
                    "security_groups"
                ],
                [
                    "security_group_ids",
                    "subnet_id"
                ]
            ]
        }
