                [
                    "aws_credential_file",
                    "ssh_keypair",
                    "ssh_key_resource",
                    "aws_region"
                ]
            ]
        },
        "user": {
            "fields": {
                "associate_public_ip": {
                    "label": "Associate public IP (VPC only)",
                    "override": false,
                    "sensitive": false,
                    "tip": "Associate public IP (VPC only)",
                    "type": "checkbox"
                },
                "availability_zone": {
                    "label": "Availability Zone",
                    "override": false,
                    "sensitive": false,
                    "tip": "AWS Availability Zone",
                    "type": "text"
                },
                "dedicated_instance": {
                    "label": "Use Dedicated instances (VPC only)",
                    "override": false,
                    "sensitive": false,
                    "tip": "Launch as dedicated instances (VPC only)",
                    "type": "checkbox"
                },
                "iam_instance_profile": {
                    "label": "IAM instance profile",
                    "override": false,
                    "sensitive": false,
                    "tip": "IAM instance profile to apply to cluster",
                    "type": "text"
                },
                "placement_group": {
                    "label": "Placement group",
                    "override": false,
                    "sensitive": false,
                    "tip": "Placement group to place cluster instances",
                    "type": "text"
