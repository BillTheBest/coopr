                    "override": false,
                    "sensitive": false,
                    "tip": "AWS Region",
                    "type": "select"
                },
                "ssh_key_resource": {
                    "label": "SSH Key Resource Name",
                    "override": true,
                    "sensitive": false,
                    "tip": "Name of the SSH private key resource uploaded to the server",
                    "type": "text"
                },
                "ssh_keypair": {
                    "label": "Key Pair Name",
                    "override": true,
                    "sensitive": false,
                    "tip": "Your AWS EC2 key pair name",
                    "type": "text"
                }
            },
            "required": [
                [
                    "api_user",
                    "api_password",
                    "ssh_keypair",
                    "ssh_key_resource",
                    "aws_region"
