{
    "description": "",
    "name": "aws",
    "parameters": {
        "admin": {
            "fields": {
                "api_password": {
                    "label": "Secret Access Key",
                    "override": false,
                    "sensitive": false,
                    "tip": "AWS Secret Access Key",
                    "type": "password"
                },
                "api_user": {
                    "label": "Access Key ID",
                    "override": false,
                    "sensitive": false,
                    "tip": "AWS Access Key ID",
                    "type": "text"
                },
                "aws_credential_file": {
                    "label": "AWS Credential file",
                    "override": true,
                    "sensitive": false,
                    "tip": "Full path and file name to AWS credential file as used by AWS command line tools",
                    "type": "text"
                },
                "aws_region": {
                    "default": "us-east-1",
                    "label": "AWS Region",
                    "options": [
                        "ap-northeast-1",
                        "ap-southeast-1",
                        "ap-southeast-2",
                        "eu-west-1",
                        "sa-east-1",
                        "us-east-1",
                        "us-west-1",
                        "us-west-2"
